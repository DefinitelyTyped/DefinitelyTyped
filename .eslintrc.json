{
    "root": true,
    "extends": [
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "dt-header": "error",
      "export-just-namespace": "error",
      "no-bad-reference": "error",
      "no-dead-reference": "error",
      "no-const-enum": "error",
      "no-useless-files": "error",
      "trim-file": "error"
    }
}
