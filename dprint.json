{
  "excludes": [
    // These files are auto-generated and don't need to be formatted
    "./types/vscode"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.85.1.wasm"
  ],
  "typescript": {
    "arrowFunction.useParentheses": "preferNone",
    "indentWidth": 4,
    "lineWidth": 120,
    "quoteStyle": "alwaysSingle",
    "trailingCommas": "onlyMultiLine"
  }
}
