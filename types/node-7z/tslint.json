{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "no-declare-current-package": false,
        "no-single-declare-module": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "space-before-function-paren": false
    }
}
