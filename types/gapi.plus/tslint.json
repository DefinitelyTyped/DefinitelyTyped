{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "no-padding": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "object-literal-key-quotes": false,
        "one-line": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "strict-export-declare-modifiers": false,
        "whitespace": false
    }
}
