{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "dt-header": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-redundant-jsdoc-2": false,
        "no-var-keyword": false,
        "object-literal-shorthand": false,
        "one-line": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "trim-file": false
    }
}
