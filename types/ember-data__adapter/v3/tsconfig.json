{
    "compilerOptions": {
        "module": "commonjs",
        "lib": ["es6", "dom"],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "ember-data": ["ember-data/v3"],
            "@ember-data/adapter": ["ember-data__adapter/v3"],
            "@ember-data/adapter/*": ["ember-data__adapter/v3/*"],
            "@ember-data/model": ["ember-data__model/v3"],
            "@ember-data/store": ["ember-data__store/v3"],
            "@ember/service": ["ember__service/v3"],
            "@ember/object": ["ember__object/v3"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "experimentalDecorators": true
    },
    "files": [
        "index.d.ts",
        "test/lib/assert.ts",
        "test/index.ts",
        "test/error.ts",
        "test/json-api.ts",
        "test/rest.ts"
    ]
}
