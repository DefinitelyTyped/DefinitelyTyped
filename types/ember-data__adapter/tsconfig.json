{
    "compilerOptions": {
        "module": "commonjs",
        "lib": ["es6", "dom"],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@ember-data/*": ["ember-data__*"],
            "@ember/*": ["ember__*"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "experimentalDecorators": true
    },
    "files": [
        "index.d.ts",
        "test/lib/assert.ts",
        "test/index.ts",
        "test/error.ts",
        "test/json-api.ts",
        "test/rest.ts"
    ]
}
