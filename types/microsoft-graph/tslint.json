{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "interface-name": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-empty-interface": false,
        "no-irregular-whitespace": false,
        "no-padding": false,
        "npm-naming": false,
        "semicolon": false
    }
}
