{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "no-var-keyword": false,
        "one-variable-per-declaration": false,
        "only-arrow-functions": false,
        "prefer-conditional-expression": false,
        "prefer-const": false,
        "prefer-for-of": false,
        "prefer-template": false,
        "triple-equals": false
    }
}
