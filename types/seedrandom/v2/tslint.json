{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-over-type-literal": false,
        "no-consecutive-blank-lines": false,
        "no-misused-new": false,
        "no-padding": false,
        "no-var-keyword": false,
        "prefer-const": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
