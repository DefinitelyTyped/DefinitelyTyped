{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "alwaysStrict": true,
        "target": "es5",
        "allowJs": true,
        "checkJs": true,
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "muv/index.d.ts",
        "muv-dom/index.d.ts",
        "muvjs-tests.ts"
    ]
}
