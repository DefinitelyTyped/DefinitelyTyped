{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "interface-name": false,
        "no-empty-interface": false,
        "no-single-declare-module": false,
        "unified-signatures": false,

        "class-name": true,
        "indent": [true, "spaces"],
        "quotemark": [true, "double"],
        "comment-format": false
    }
}
