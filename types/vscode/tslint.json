{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "jsdoc-format": false,
        "no-irregular-whitespace": false,
        "no-redundant-undefined": false,
        "no-unnecessary-class": false,
        "npm-naming": false,
        "unified-signatures": false
    }
}
