{
    "extends": "dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-unnecessary-qualifier": false,
        "semicolon": false,
        "space-within-parens": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
