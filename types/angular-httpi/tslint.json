{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-unnecessary-generics": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
