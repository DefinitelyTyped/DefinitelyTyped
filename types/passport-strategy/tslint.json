{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "no-redundant-jsdoc-2": false,
        "no-reference-import": false,
        "no-var-keyword": false,
        "no-void-expression": false,
        "npm-naming": false,
        "prefer-const": false,
        "triple-equals": false
    }
}
