{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "jsdoc-format": false,
        "member-access": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-trailing-whitespace": false,
        "space-before-function-paren": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
