{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-declare-current-package": false,
        "no-single-declare-module": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "prefer-for-of": false,
        "triple-equals": false,
        "unified-signatures": false
    }
}
