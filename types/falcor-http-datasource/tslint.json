{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-reference-import": false,
        "npm-naming": [
            true,
            {
                "errors": [["NeedsExportEquals", false]],
                "mode": "code"
            }
        ],
        "trim-file": false,
        "whitespace": false
    }
}
