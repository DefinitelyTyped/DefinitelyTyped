{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "jsdoc-format": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "semicolon": false
    }
}
