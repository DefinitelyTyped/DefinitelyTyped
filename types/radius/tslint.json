{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-inferrable-types": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "object-literal-shorthand": false,
        "prefer-const": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "triple-equals": false
    }
}
