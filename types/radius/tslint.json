{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "jsdoc-format": false,
        "no-inferrable-types": false,
        "no-var-keyword": false,
        "object-literal-shorthand": false,
        "prefer-const": false,
        "triple-equals": false
    }
}
