{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-inferrable-types": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [["NeedsExportEquals", false]],
                "mode": "code"
            }
        ],
        "prefer-const": false,
        "typedef-whitespace": false,
        "unified-signatures": false
    }
}
