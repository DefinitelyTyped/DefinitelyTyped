{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-inferrable-types": false,
        "no-padding": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [
                    [
                        "NeedsExportEquals",
                        false
                    ]
                ],
                "mode": "code"
            }
        ],
        "prefer-const": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "unified-signatures": false
    }
}
