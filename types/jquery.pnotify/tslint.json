{
    "extends": "dtslint/dt.json",
    "rules": {
        "jsdoc-format": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-object-literal-type-assertion": false,
        "no-padding": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "object-literal-key-quotes": false,
        "prefer-const": false,
        "semicolon": false
    }
}
