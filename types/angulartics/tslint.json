{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "no-namespace": false,
        "prefer-const": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
