{
    "extends": "dtslint/dt.json",
    "rules": {
        "callable-types": false,
        "comment-format": false,
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "no-namespace": false,
        "no-padding": false,
        "no-redundant-jsdoc": false,
        "prefer-const": false,
        "prefer-method-signature": false,
        "semicolon": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}