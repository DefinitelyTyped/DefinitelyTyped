{
    "extends": "dtslint/dt.json",
    "rules": {
        // All are TODOs
        "comment-format": [false],
        "jsdoc-format": false,
        "max-line-length": [false],
        "no-any-union": false,
        "no-empty-interface": false,
        "no-namespace": false,
        "no-unnecessary-generics": false,
        "prefer-const": false,
        "typedef-whitespace": [false],
        "unified-signatures": false,
        "whitespace": [false]
    }
}
