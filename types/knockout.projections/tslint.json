{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "interface-name": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "space-before-function-paren": false,
        "unified-signatures": false
    }
}
