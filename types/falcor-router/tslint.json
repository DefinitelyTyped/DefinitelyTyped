{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-reference-import": false,
        "semicolon": false,
        "trim-file": false,
        "whitespace": false
    }
}
