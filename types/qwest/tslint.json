{
    "extends": "dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "dt-header": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-declare-current-package": false,
        "no-padding": false,
        "no-single-declare-module": false,
        "only-arrow-functions": false,
        "semicolon": false,
        "trim-file": false,
        "typedef-whitespace": false
    }
}
