{
    "compilerOptions": {
        "module": "commonjs",
        "lib": ["es6", "dom"],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noEmit": true,
        "jsx": "preserve",
        "jsxFactory": "preact.h",
        "baseUrl": ".",
        "paths": {
            "@storybook/preact": [
                "storybook__preact"
            ]
        },
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "storybook__preact-tests.tsx"
    ]
}
