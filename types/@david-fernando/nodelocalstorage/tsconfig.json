{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2019",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true
  },
  "include": [
    "*.ts"
  ]
}