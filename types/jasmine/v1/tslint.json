{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "file-name-casing": [
            true,
            "kebab-case"
        ],
        "no-unnecessary-type-assertion": false,
        "no-var-keyword": false,
        "one-variable-per-declaration": false,
        "prefer-const": false,
        "unified-signatures": false,
        "void-return": false
    }
}
