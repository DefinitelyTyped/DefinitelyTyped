{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "class-name": true,
        "jsdoc-format": false,
        "no-const-enum": false,
        "no-empty-interface": false,
        "no-unnecessary-type-assertion": false,
        "npm-naming": false,
        "quotemark": [true, "double"],
        "unified-signatures": false
    }
}
