{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "object-literal-key-quotes": false,
        "only-arrow-functions": false,
        "prefer-template": false,
        "space-before-function-paren": false,
        "trim-file": false
    }
}
