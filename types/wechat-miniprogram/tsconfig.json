{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": ["es6"],
    "module": "CommonJS",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmit": true,
    "pretty": true,
    "removeComments": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "target": "ES5",
    "typeRoots": [ "../" ],
    "types": [],
    "baseUrl": "../",
  },
  "files": [
    "index.d.ts",
    "./wx/index.d.ts",
    "./wx/lib.wx.api.d.ts",
    "./wx/lib.wx.app.d.ts",
    "./wx/lib.wx.behavior.d.ts",
    "./wx/lib.wx.cloud.d.ts",
    "./wx/lib.wx.component.d.ts",
    "./wx/lib.wx.event.d.ts",
    "./wx/lib.wx.page.d.ts",
  ]
