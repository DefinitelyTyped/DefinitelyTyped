{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-trailing-whitespace": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "semicolon": false,
        "trim-file": false
    }
}
