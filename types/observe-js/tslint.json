{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "jsdoc-format": false,
        "no-for-in-array": false,
        "no-namespace": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "prefer-for-of": false,
        "prefer-template": false
    }
}
