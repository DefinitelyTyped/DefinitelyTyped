{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "jsx": "react",
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "history": [
                "history/v3"
            ],
            "history/*": [
                "history/v3/*"
            ],
            "react-router": [
                "react-router/v3"
            ],
            "react-router/*": [
                "react-router/v3/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "react-router-tests.tsx"
    ]
}
