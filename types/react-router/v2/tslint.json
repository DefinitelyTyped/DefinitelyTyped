{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-unnecessary-generics": false,
        "no-unnecessary-qualifier": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
