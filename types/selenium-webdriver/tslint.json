{
    "extends": "dtslint/dt.json",
    "rules": {
        "align": false,
        "ban-types": false,
        "callable-types": false,
        "interface-name": [false],
        "jsdoc-format": false,
        "no-empty-interface": false,
        "semicolon": false,
        "unified-signatures": false
    }
}
