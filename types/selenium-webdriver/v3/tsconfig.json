{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": false,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "selenium-webdriver": [
                "selenium-webdriver/v3"
            ],
            "selenium-webdriver/*": [
                "selenium-webdriver/v3/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "test/index.ts",
        "test/chrome.ts",
        "test/firefox.ts",
        "test/remote.ts",
        "test/testing.ts"
    ]
}
