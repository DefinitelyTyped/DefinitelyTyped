{
    "files": [
        "index.d.ts",
        "module-tests.ts"
    ],
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es2018",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "module": [
                "module/v13"
            ],
            "querystring": [
                "querystring/v13"
            ],
            "url": [
                "url/v13"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    }
}
