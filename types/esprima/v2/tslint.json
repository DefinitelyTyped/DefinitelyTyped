{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "export-just-namespace": false,
        "no-padding": false,
        "no-reference-import": false,
        "no-var-keyword": false,
        "one-line": false,
        "prefer-const": false,
        "semicolon": false,
        "whitespace": false
    }
}
