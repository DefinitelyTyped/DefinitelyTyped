{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "../",
    "typeRoots": ["../"],
    "types": []
  }
}
