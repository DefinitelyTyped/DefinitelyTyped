{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "interface-name": false,
        "interface-over-type-literal": false,
        "no-empty-interface": false,
        "no-unnecessary-qualifier": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "unified-signatures": false
    }
}
