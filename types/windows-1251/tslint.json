{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-inferrable-types": false,
        "no-padding": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "one-variable-per-declaration": false,
        "prefer-const": false,
        "semicolon": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
