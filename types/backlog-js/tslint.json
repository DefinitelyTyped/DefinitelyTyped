{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "interface-over-type-literal": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-unnecessary-generics": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false
    }
}
