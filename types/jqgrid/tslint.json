{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "comment-format": false,
        "dt-header": false,
        "interface-name": false,
        "max-line-length": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "unified-signatures": false
    }
}
