{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "interface-name": false,
        "no-duplicate-variable": false,
        "no-empty-interface": false,
        "no-internal-module": false,
        "no-var-keyword": false,
        "prefer-const": false
    }
}
