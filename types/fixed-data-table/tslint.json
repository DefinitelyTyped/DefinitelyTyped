{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "export-just-namespace": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-reference-import": false,
        "no-var-keyword": false,
        "strict-export-declare-modifiers": false
    }
}
