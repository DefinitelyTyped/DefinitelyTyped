{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "export-just-namespace": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "prefer-const": false,
        "space-before-function-paren": false,
        "trim-file": false
    }
}
