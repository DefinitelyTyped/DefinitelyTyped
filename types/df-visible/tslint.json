{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-padding": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "one-line": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "space-within-parens": false,
        "triple-equals": false,
        "whitespace": false
    }
}
