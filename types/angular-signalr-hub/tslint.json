{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "member-access": false,
        "no-declare-current-package": false,
        "no-single-declare-module": false,
        "no-var-keyword": false,
        "object-literal-key-quotes": false,
        "prefer-const": false,
        "prefer-for-of": false,
        "semicolon": false,
        "whitespace": false
    }
}
