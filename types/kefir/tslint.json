{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "comment-format": false,
        "max-line-length": false,
        "no-duplicate-imports": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "void-return": false,
        "whitespace": false
    }
}
