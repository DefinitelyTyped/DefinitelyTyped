{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-over-type-literal": false,
        "no-empty-interface": false,
        "no-irregular-whitespace": false,
        "no-padding": false,
        "one-line": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "space-before-function-paren": false,
        "whitespace": false
    }
}
