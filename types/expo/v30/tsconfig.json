{
    "compilerOptions": {
        "module": "commonjs",
        "lib": ["es6"],
        "jsx": "react",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "expo": [
                "expo/v30"
            ],
            "expo/*": [
                "expo/v30/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "expo-tests.tsx"
    ]
}
