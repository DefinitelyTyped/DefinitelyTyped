{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-misused-new": false,
        "no-redundant-jsdoc-2": false,
        "no-single-declare-module": false,
        "npm-naming": [
            true,
            {
                "errors": [["NeedsExportEquals", false]],
                "mode": "code"
            }
        ],
        "unified-signatures": false
    }
}
