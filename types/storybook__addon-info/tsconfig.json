{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "dom",
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "jsx": "react",
        "typeRoots": ["../"],
        "paths": {
            "@storybook/*": [
                "storybook__*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true
    },
    "files": [
        "index.d.ts",
        "storybook__addon-info-tests.tsx"
    ]
}
