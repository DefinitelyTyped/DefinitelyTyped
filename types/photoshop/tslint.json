{ 
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "strict-export-declare-modifiers": false,
        "no-redundant-jsdoc": false,
        "void-return": false,
        "array-type": false,
        "npm-naming": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-unnecessary-class": false,
        "unified-signatures": false
    }
}
