{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "comment-format": false,
        "no-consecutive-blank-lines": false,
        "no-internal-module": false,
        "no-single-declare-module": false,
        "prefer-const": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
