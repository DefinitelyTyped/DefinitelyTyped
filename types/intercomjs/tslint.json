{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "semicolon": false,
        "whitespace": false
    }
}
