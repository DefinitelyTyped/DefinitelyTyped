{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": false,
        "strictNullChecks": true,
        "strictFunctionTypes": false,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [
            
        ],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@feathersjs/express": [
                "feathersjs__express"
            ],
            "@feathersjs/feathers": [
                "feathersjs__feathers"
            ],
            "@feathersjs/socketio": [
                "feathersjs__socketio"
            ]
        }
    }
}