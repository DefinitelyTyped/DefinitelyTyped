{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "member-access": false,
        "no-inferrable-types": false,
        "no-var-keyword": false,
        "one-line": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "prefer-template": false,
        "semicolon": false,
        "space-before-function-paren": false
    }
}
