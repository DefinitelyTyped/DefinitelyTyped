{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "space-before-function-paren": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false,
        "typedef-whitespace": false
    }
}
