{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "@ember/engine": ["ember__engine/v3"],
            "@ember/engine/*": ["ember__engine/v3/*"],
            "@ember/object": ["ember__object/v3"],
            "@ember/object/*": ["ember__object/v3/*"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "test/engine.ts",
        "test/engine-instance.ts"
    ]
}
