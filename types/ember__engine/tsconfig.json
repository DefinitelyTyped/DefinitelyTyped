{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@ember/object": [
                "ember__object"
            ],
            "@ember/object/*": [
                "ember__object/*"
            ],
            "@ember/engine": [
                "ember__engine"
            ],
            "@ember/engine/*": [
                "ember__engine/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "test/engine.ts",
        "test/engine-instance.ts"
    ]
}
