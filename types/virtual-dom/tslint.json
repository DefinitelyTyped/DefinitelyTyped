{
    "extends": "dtslint/dt.json",
    "rules": {
        "callable-types": false,
        "dt-header": false,
        "jsdoc-format": false,
        "no-any-union": false,
        "no-declare-current-package": false,
        "no-redundant-jsdoc": false,
        "no-var-keyword": false,
        "one-line": false,
        "prefer-const": false,
        "prefer-method-signature": false
    }
}