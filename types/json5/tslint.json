{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "comment-format": false,
        "dt-header": false,
        "jsdoc-format": false,
        "no-any-union": false,
        "no-consecutive-blank-lines": false,
        "no-trailing-whitespace": false,
        "trim-file": false,
        "unified-signatures": false
    }
}
