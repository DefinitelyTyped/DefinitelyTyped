{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target":"es6",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "jsx": "react",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@hcaptcha/react-hcaptcha": [
                "hcaptcha__react-hcaptcha"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "hcaptcha__react-hcaptcha-tests.tsx"
    ]
}
