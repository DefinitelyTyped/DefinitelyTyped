{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "no-redundant-undefined": false,
        "no-unnecessary-class": false,
        "no-unnecessary-qualifier": false,
        "no-void-expression": false,
        "only-arrow-functions": false,
        "prefer-const": false
    }
}
