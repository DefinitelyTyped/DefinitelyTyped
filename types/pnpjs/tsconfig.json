{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "experimentalDecorators": true,
        "paths": {
            "@pnp/*": [
                "pnpjs/v2/*"
            ],
            "@pnp": [
                "pnpjs/v2/pnpjs"
            ]
        }
    },
    "files": [
        "index.d.ts",
        "pnpjs-tests.ts"
    ]
}