{
    "extends": "dtslint/dt.json",
    "rules": {
        "align": false,
        "array-type": false,
        "interface-over-type-literal": false,
        "new-parens": false,
        "no-consecutive-blank-lines": false,
        "no-unnecessary-generics": false,
        "no-var": false,
        "prefer-declare-function": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}