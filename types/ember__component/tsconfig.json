{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@ember/component": ["ember__component"],
            "@ember/component/*": ["ember__component/*"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "checkbox.d.ts",
        "text-area.d.ts",
        "text-field.d.ts",
        "helper.d.ts",
        "test/lib/assert.ts",
        "test/component.ts",
        "test/helper.ts"
    ]
}
