{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@ember/object": [
                "ember__object"
            ],
            "@ember/object/*": [
                "ember__object/*"
            ],
            "@ember/controller": [
                "ember__controller"
            ],
            "@ember/controller/*": [
                "ember__controller/*"
            ],
            "@ember/component": [
                "ember__component"
            ],
            "@ember/component/*": [
                "ember__component/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "test/lib/assert.ts",
        "test/component.ts",
        "test/helper.ts"
    ]
}