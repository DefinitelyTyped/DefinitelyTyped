{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "dt-header": false,
        "jsdoc-format": false,
        "no-declare-current-package": false,
        "no-padding": false,
        "no-single-declare-module": false,
        "no-var-keyword": false,
        "one-line": false,
        "only-arrow-functions": false,
        "prefer-const": false
    }
}
