{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom",
            "es2017"
        ],
        "target": "es2017",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "puppeteer": [
                "puppeteer/v2"
            ],
            "puppeteer/*": [
                "puppeteer/v2/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "puppeteer-tests.ts"
    ]
}
