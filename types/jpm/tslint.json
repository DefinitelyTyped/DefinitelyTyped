{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "interface-over-type-literal": false,
        "jsdoc-format": false,
        "no-empty-interface": false,
        "no-misused-new": false,
        "no-redundant-jsdoc-2": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "prefer-const": false,
        "triple-equals": false
    }
}
