{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [
                    [
                        "NeedsExportEquals",
                        false
                    ]
                ],
                "mode": "code"
            }
        ],
        "prefer-const": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
