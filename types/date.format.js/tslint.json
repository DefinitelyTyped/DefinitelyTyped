{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-redundant-jsdoc-2": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "space-within-parens": false,
        "trim-file": false,
        "typedef-whitespace": false
    }
}
