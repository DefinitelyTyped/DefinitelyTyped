{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "interface-name": false,
        "member-access": false,
        "new-parens": false,
        "no-redundant-jsdoc-2": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [["NeedsExportEquals", false]],
                "mode": "code"
            }
        ],
        "prefer-const": false,
        "triple-equals": false
    }
}
