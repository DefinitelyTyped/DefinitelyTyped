{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-redundant-undefined": false,
        "no-trailing-whitespace": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "prefer-const": false,
        "trim-file": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
