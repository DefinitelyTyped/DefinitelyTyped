{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "interface-name": false,
        "no-misused-new": false,
        "no-padding": false,
        "no-var-keyword": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "typedef-whitespace": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
