{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "interface-name": false,
        "no-redundant-jsdoc-2": false,
        "no-reference-import": false,
        "no-unnecessary-qualifier": false,
        "one-variable-per-declaration": false,
        "prefer-const": false,
        "prefer-template": false
    }
}
