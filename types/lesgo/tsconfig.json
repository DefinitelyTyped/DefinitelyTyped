{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "Middlewares/*": [
                "lesgo/middlewares/*"
            ],
            "Exceptions/*": [
                "lesgo/exceptions/*"
            ],
            "Utils/*": [
                "lesgo/utils/*"
            ],
            "Services/*": [
                "lesgo/services/*"
            ],
            "Tests/*": [
                "lesgo/tests/*"
            ]
        }
    },
    "files": [
        "index.d.ts",
        "lesgo-tests.ts"
    ]
}
