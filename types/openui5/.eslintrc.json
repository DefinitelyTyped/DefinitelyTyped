{
    "rules": {
        "@definitelytyped/no-any-union": "off",
        "@definitelytyped/no-unnecessary-generics": "off",
        "jsdoc/check-tag-names": [
            "error",
            {
                "definedTags": ["ui5-protected", "experimental"]
            }
        ],
        "@definitelytyped/strict-export-declare-modifiers": "off"
    }
}
