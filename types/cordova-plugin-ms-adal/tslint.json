{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "one-line": false,
        "prefer-const": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
