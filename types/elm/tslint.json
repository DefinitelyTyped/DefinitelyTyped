{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "no-consecutive-blank-lines": false,
        "no-empty-interface": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "semicolon": false,
        "trim-file": false
    }
}
