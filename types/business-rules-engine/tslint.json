{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "interface-name": false,
        "member-access": false,
        "no-consecutive-blank-lines": false,
        "no-unnecessary-class": false,
        "no-var-keyword": false,
        "one-line": false,
        "prefer-const": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
