{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "adjacent-overload-signatures": false,
        "ban-types": false,
        "comment-format": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-var-keyword": false,
        "prefer-const": false,
        "unified-signatures": false
    }
}
