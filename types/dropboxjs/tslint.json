{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "one-line": false,
        "prefer-template": false,
        "space-within-parens": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
