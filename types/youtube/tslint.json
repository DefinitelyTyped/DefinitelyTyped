{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-name": false,
        "no-const-enum": false,
        "no-trailing-whitespace": false,
        "one-line": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "unified-signatures": false
    }
}
