{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "comment-format": false,
        "dt-header": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-void-expression": false,
        "strict-export-declare-modifiers": false,
        "unified-signatures": false
    }
}
