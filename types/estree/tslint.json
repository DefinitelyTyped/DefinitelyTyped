{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "dt-header": false,
        "no-duplicate-variable": false,
        "no-empty-interface": false,
        "no-unnecessary-type-assertion": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false
    }
}
