{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "adjacent-overload-signatures": false,
        "ban-types": false,
        "jsdoc-format": false,
        "no-object-literal-type-assertion": false,
        "no-unnecessary-generics": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false
    }
}
