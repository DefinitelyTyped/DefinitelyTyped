{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "jsx": "react",
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "types": [],
        "paths": {
            "redux-form": [
                "redux-form/v6"
            ],
            "redux-form/*": [
                "redux-form/v6/*"
            ]
        },
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "redux-form-tests.tsx"
    ]
}
