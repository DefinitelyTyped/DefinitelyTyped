{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "target": "ES6",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@foursquare/foursquare-places": [
                "foursquare__foursquare-places"
            ]
        }
    },
    "files": [
        "index.d.ts",
        "foursquare__foursquare-places-tests.ts"
    ]
}
