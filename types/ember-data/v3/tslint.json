{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "jsdoc-format": false,
        "no-empty-interface": false,
        "no-return-await": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "npm-naming": [
            true,
            {
                "mode": "code",
                "errors": [["NoDefaultExport", false]]
            }
        ]
    }
}
