{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "no-redundant-jsdoc-2": false,
        "no-unnecessary-class": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [
                    [
                        "NeedsExportEquals",
                        false
                    ]
                ],
                "mode": "code"
            }
        ],
        "only-arrow-functions": false,
        "prefer-const": false,
        "space-before-function-paren": false
    }
}
