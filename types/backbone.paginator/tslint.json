{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-namespace": false,
        "no-object-literal-type-assertion": false,
        "no-var-keyword": false,
        "one-line": false,
        "prefer-const": false,
        "semicolon": false,
        "space-within-parens": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
