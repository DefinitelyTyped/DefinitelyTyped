{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "import-spacing": false,
        "interface-name": false,
        "npm-naming": false,
        "only-arrow-functions": false,
        "strict-export-declare-modifiers": false,
        "triple-equals": false,
        "unified-signatures": false
    }
}
