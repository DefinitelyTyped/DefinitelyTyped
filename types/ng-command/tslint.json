{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-name": false,
        "jsdoc-format": false,
        "no-boolean-literal-compare": false,
        "no-padding": false,
        "no-unnecessary-class": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "semicolon": false
    }
}
