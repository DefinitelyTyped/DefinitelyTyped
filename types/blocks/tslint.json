{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "jsdoc-format": false,
        "no-construct": false,
        "no-duplicate-variable": false,
        "no-var-keyword": false,
        "object-literal-shorthand": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "prefer-template": false,
        "triple-equals": false,
        "unified-signatures": false
    }
}
