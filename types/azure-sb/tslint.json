{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "interface-name": false,
        "member-access": false,
        "no-empty-interface": false,
        "no-padding": false,
        "no-self-import": false,
        "no-unnecessary-generics": false,
        "object-literal-key-quotes": false,
        "strict-export-declare-modifiers": false,
        "whitespace": false
    }
}
