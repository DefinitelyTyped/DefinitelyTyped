{
    "extends": "dtslint/dt.json",
    "rules": {
        "adjacent-overload-signatures": false,
        "ban-types": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-internal-module": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
