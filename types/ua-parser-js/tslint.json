{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "interface-name": false,
        "no-declare-current-package": false,
        "no-duplicate-variable": false,
        "no-padding": false,
        "no-single-declare-module": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [
                    [
                        "NeedsExportEquals",
                        false
                    ]
                ],
                "mode": "code"
            }
        ],
        "prefer-const": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false
    }
}
