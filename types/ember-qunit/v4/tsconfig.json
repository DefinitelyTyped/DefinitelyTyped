{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "strict": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "ember": ["ember/v3"],
            "ember-qunit": ["ember-qunit/v4"],
            "@ember/application": ["ember__application/v3"],
            "@ember/application/*": ["ember__application/v3/*"],
            "@ember/array": ["ember__array/v3"],
            "@ember/array/*": ["ember__array/v3/*"],
            "@ember/component": ["ember__component/v3"],
            "@ember/component/*": ["ember__component/v3/*"],
            "@ember/controller": ["ember__controller/v3"],
            "@ember/debug": ["ember__debug/v3"],
            "@ember/debug/*": ["ember__debug/v3/*"],
            "@ember/engine": ["ember__engine/v3"],
            "@ember/engine/*": ["ember__engine/v3/*"],
            "@ember/error": ["ember__error/v3"],
            "@ember/object": ["ember__object/v3"],
            "@ember/object/*": ["ember__object/v3/*"],
            "@ember/polyfills": ["ember__polyfills/v3"],
            "@ember/routing": ["ember__routing/v3"],
            "@ember/routing/*": ["ember__routing/v3/*"],
            "@ember/runloop": ["ember__runloop/v3"],
            "@ember/runloop/*": ["ember__runloop/v3/*"],
            "@ember/service": ["ember__service/v3"],
            "@ember/string": ["ember__string/v2"],
            "@ember/template": ["ember__template/v3"],
            "@ember/template/*": ["ember__template/v3/*"],
            "@ember/test": ["ember__test/v3"],
            "@ember/test/*": ["ember__test/v3/*"],
            "@ember/utils": ["ember__utils/v3"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "ember-qunit-tests.ts"
    ]
}
