{
    "extends": "dtslint/dt.json",
    "rules": {
        "adjacent-overload-signatures": true,
        "array-type": true,
        "arrow-return-shorthand": true,
        "ban-types": true,
        "callable-types": true,
        "comment-format": true,
        "dt-header": true,
        "eofline": true,
        "export-just-namespace": true,
        "import-spacing": true,
        "interface-name": true,
        "interface-over-type-literal": true,
        "jsdoc-format": true,
        "max-line-length": true,
        "member-access": true,
        "new-parens": true,
        "no-any-union": true,
        "no-boolean-literal-compare": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-construct": true,
        "no-declare-current-package": true,
        "no-duplicate-imports": true,
        "no-duplicate-variable": true,
        "no-empty-interface": true,
        "no-for-in-array": true,
        "no-inferrable-types": true,
        "no-internal-module": true,
        "no-irregular-whitespace": true,
        "no-mergeable-namespace": true,
        "no-misused-new": true,
        "no-namespace": true,
        "no-object-literal-type-assertion": true,
        "no-padding": true,
        "no-redundant-jsdoc": true,
        "no-redundant-jsdoc-2": true,
        "no-redundant-undefined": true,
        "no-reference-import": true,
        "no-relative-import-in-test": true,
        "no-self-import": true,
        "no-single-declare-module": true,
        "no-string-throw": true,
        "no-unnecessary-callback-wrapper": true,
        "no-unnecessary-class": true,
        "no-unnecessary-generics": true,
        "no-unnecessary-qualifier": true,
        "no-unnecessary-type-assertion": true,
        "no-useless-files": true,
        "no-var-keyword": true,
        "no-var-requires": true,
        "no-void-expression": true,
        "no-trailing-whitespace": true,
        "object-literal-key-quotes": true,
        "object-literal-shorthand": true,
        "one-line": true,
        "one-variable-per-declaration": true,
        "only-arrow-functions": true,
        "prefer-conditional-expression": true,
        "prefer-const": true,
        "prefer-declare-function": true,
        "prefer-for-of": true,
        "prefer-method-signature": true,
        "prefer-object-spread": true,
        "prefer-template": true,
        "radix": true,
        "semicolon": true,
        "space-before-function-paren": true,
        "space-within-parens": true,
        "strict-export-declare-modifiers": true,
        "trim-file": true,
        "triple-equals": true,
        "typedef-whitespace": true,
        "unified-signatures": true,
        "use-default-type-parameter": true,
        "void-return": true,
        "whitespace": true
    }
}