{
    "extends": "dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "dt-header": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-padding": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "semicolon": false,
        "whitespace": false
    }
}
