{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-duplicate-variable": false,
        "no-padding": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "whitespace": false
    }
}
