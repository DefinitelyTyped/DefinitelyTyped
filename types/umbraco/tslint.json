{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "interface-name": false,
        "jsdoc-format": false,
        "no-empty-interface": false,
        "no-redundant-jsdoc-2": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false
    }
}
