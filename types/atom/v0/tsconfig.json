{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "strictFunctionTypes": false,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "atom": [
                "atom/v0"
            ],
            "pathwatcher": [
