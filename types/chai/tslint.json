{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "callable-types": false,
        "new-parens": false,
        "no-construct": false,
        "no-empty-interface": false,
        "no-inferrable-types": false,
        "no-redundant-jsdoc-2": false,
        "no-unnecessary-generics": false
    }
}
