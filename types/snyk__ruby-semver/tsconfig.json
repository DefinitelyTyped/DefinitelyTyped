{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@snyk/ruby-semver": [
                "snyk__ruby-semver"
            ],
            "@snyk/ruby-semver/*": [
                "snyk__ruby-semver/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "snyk__ruby-semver-tests.ts"
    ]
}
