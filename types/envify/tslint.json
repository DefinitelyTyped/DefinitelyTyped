{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-declare-current-package": false,
        "no-var-keyword": false,
        "one-variable-per-declaration": false,
        "prefer-const": false,
        "prefer-declare-function": false,
        "trim-file": false
    }
}
