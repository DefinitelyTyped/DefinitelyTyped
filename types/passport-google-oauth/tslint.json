{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "dt-header": false,
        "interface-name": false,
        "no-reference-import": false,
        "no-var-keyword": false,
        "no-void-expression": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "strict-export-declare-modifiers": false
    }
}
