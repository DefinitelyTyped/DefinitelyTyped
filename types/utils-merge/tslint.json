{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-over-type-literal": false,
        "no-consecutive-blank-lines": false,
        "no-unnecessary-generics": false,
        "npm-naming": [
            true,
            {
                "mode": "name-only"
            }
        ],
        "trim-file": false
    }
}
