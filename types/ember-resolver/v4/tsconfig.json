{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": false,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "ember": ["ember/v2"],
            "ember-resolver": ["ember-resolver/v4"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "ember-resolver-tests.ts"
    ]
}
