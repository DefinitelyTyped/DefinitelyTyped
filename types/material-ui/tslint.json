{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "no-any-union": false,
        "no-declare-current-package": false,
        "no-duplicate-imports": false,
        "no-empty-interface": false,
        "no-mergeable-namespace": false,
        "strict-export-declare-modifiers": false,
        "npm-naming": false
    }
}