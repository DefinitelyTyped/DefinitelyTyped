{
    "compilerOptions": {
      "module": "commonjs",
      "lib": [
        "es6",
        "dom"
      ],
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "baseUrl": "../",
      "typeRoots": [
        "../"
      ],
      "types": [

      ],
      "paths": {
        "puppeteer": [
            "puppeteer/v3"
        ],
        "puppeteer/*": [
            "puppeteer/v3/*"
        ]
      },
      "noEmit": true,
      "forceConsistentCasingInFileNames": true
    },
    "files": [
      "index.d.ts",
      "pa11y-tests.ts"
    ]
}
