{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "comment-format": false,
        "no-boolean-literal-compare": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "semicolon": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
