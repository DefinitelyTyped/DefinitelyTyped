{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "one-line": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "triple-equals": false,
        "whitespace": false
    }
}
