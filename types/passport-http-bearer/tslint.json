{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "interface-name": false,
        "member-access": false,
        "no-reference-import": false,
        "object-literal-shorthand": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "strict-export-declare-modifiers": false
    }
}
