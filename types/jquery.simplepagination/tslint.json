{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-redundant-jsdoc-2": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "only-arrow-functions": false,
        "semicolon": false,
        "space-before-function-paren": false,
        "unified-signatures": false
    }
}
