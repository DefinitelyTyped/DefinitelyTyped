{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "comment-format": false,
        "jsdoc-format": false,
        "no-duplicate-variable": false,
        "no-mergeable-namespace": false,
        "no-redundant-jsdoc-2": false,
        "no-unnecessary-class": false,
        "no-var-keyword": false,
        "npm-naming": false
    }
}
