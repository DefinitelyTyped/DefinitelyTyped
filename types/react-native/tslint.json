{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "interface-over-type-literal": false,
        "jsdoc-format": false,
        "no-empty-interface": false,
        "no-misused-new": false,
        "no-single-declare-module": false,
        "use-default-type-parameter": false,
        "interface-name": false
    }
}
