{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "no-duplicate-variable": false,
        "no-padding": false,
        "no-var-keyword": false,
        "prefer-const": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "unified-signatures": false,
        "void-return": false,
        "whitespace": false
    }
}
