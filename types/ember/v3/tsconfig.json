            "@ember/test": ["ember__test/v3"],
            "@ember/test/*": ["ember__test/v3/*"],
            "@ember/utils": ["ember__utils/v3"],
            "@ember/utils/*": ["ember__utils/v3/*"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
