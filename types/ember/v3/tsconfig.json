            "@ember/debug": ["ember__debug/v3"],
            "@ember/debug/*": ["ember__debug/v3/*"],
            "@ember/engine": ["ember__engine/v3"],
            "@ember/engine/*": ["ember__engine/v3/*"],
            "@ember/error": ["ember__error/v3"],
            "@ember/error/*": ["ember__error/v3/*"],
            "@ember/object": ["ember__object/v3"],
            "@ember/object/*": ["ember__object/v3/*"],
