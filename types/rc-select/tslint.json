{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "callable-types": false,
        "dt-header": false,
        "no-padding": false,
        "no-redundant-jsdoc": false,
        "no-reference-import": false,
        "prefer-const": false,
        "prefer-method-signature": false,
        "strict-export-declare-modifiers": false,
        "typedef-whitespace": false
    }
}