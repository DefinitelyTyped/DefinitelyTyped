{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@ember/object": [
                "ember__object"
            ],
            "@ember/object/*": [
                "ember__object/*"
            ],
            "@ember/array": [
                "ember__array"
            ],
            "@ember/array/*": [
                "ember__array/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "test/lib/assert.ts",
        "test/array.ts",
        "test/array-ext.ts",
        "test/array-proxy.ts"
    ]
}
