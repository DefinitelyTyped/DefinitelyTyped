{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "jsdoc-format": false,
        "no-redundant-jsdoc-2": false,
        "no-trailing-whitespace": false,
        "npm-naming": false,
        "typedef-whitespace": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
