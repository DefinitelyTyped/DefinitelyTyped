{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-duplicate-variable": false,
        "no-inferrable-types": false,
        "no-redundant-jsdoc-2": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "trim-file": false,
        "whitespace": false
    }
}
