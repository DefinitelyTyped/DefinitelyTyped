{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "member-access": false,
        "new-parens": false,
        "no-declare-current-package": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-single-declare-module": false,
        "no-trailing-whitespace": false,
        "one-line": false,
        "prefer-const": false,
        "prefer-template": false,
        "strict-export-declare-modifiers": false,
        "whitespace": false
    }
}
