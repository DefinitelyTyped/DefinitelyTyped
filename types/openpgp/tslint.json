{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [["NeedsExportEquals", false]],
                "mode": "code"
            }
        ],
        "only-arrow-functions": false,
        "prefer-const": false
    }
}
