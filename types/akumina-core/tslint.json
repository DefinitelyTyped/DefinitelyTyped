{
    "extends": "dtslint/dt.json",
    "rules": {
        "interface-name": false,
        "npm-naming": false,
        "dt-header": false,
        "no-padding": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-unnecessary-class": false,
        "unified-signatures": false,
        "no-mergeable-namespace": false,
        "export-just-namespace": false
    }
}
