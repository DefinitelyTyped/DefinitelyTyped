{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom",
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [
            "node"
        ],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@onflow/fcl": [
                "onflow__fcl"
            ]
        }
    },
    "files": [
        "index.d.ts",
        "onflow__fcl-tests.ts"
    ]
}
