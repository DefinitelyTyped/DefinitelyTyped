{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-empty-interface": false,
        "no-padding": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "semicolon": false,
        "trim-file": false
    }
}
