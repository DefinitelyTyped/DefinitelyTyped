{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-reference-import": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "whitespace": false
    }
}
