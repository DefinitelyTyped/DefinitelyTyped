{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "interface-name": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-reference-import": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [
                    [
                        "NeedsExportEquals",
                        false
                    ]
                ],
                "mode": "code"
            }
        ],
        "semicolon": false,
        "strict-export-declare-modifiers": false
    }
}
