{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-single-declare-module": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "prefer-template": false,
        "semicolon": false,
        "trim-file": false,
        "triple-equals": false
    }
}
