{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "export-just-namespace": false,
        "interface-name": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
