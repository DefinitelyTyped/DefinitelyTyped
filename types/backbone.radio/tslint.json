{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "object-literal-key-quotes": false,
        "prefer-const": false,
        "typedef-whitespace": false
    }
}
