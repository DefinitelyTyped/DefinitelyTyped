{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-inferrable-types": false,
        "no-var-keyword": false,
        "one-line": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false,
        "whitespace": false
    }
}
