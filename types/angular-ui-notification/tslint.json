{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-name": false,
        "max-line-length": false,
        "no-reference-import": false,
        "npm-naming": [
            true,
            {
                "errors": [["NeedsExportEquals", false]],
                "mode": "code"
            }
        ],
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
