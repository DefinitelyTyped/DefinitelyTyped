{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "interface-name": false,
        "max-line-length": false,
        "no-reference-import": false,
        "npm-naming": [
            true,
            {
                "errors": [
                    [
                        "NeedsExportEquals",
                        false
                    ]
                ],
                "mode": "code"
            }
        ],
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
