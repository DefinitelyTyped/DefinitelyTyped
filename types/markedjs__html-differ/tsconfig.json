{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@markedjs/html-differ": [
                "markedjs__html-differ"
            ],
            "@markedjs/html-differ/*": [
                "markedjs__html-differ/*"
            ]
        }
    },
    "files": [
        "index.d.ts",
        "markedjs__html-differ-tests.ts"
    ]
}
