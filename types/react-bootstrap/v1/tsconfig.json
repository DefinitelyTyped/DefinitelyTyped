{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "jsx": "react",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "react-bootstrap": [
                "react-bootstrap/v1"
            ],
            "react-bootstrap/*": [
                "react-bootstrap/v1/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": false
    },
    "files": [
        "index.d.ts"
    ]
}
