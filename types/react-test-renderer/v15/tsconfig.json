{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "paths": {
            "react": [
                "react/v15"
            ],
            "react-test-renderer": [
                "react-test-renderer/v15"
            ]
        },
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true
    },
    "files": [
        "index.d.ts",
        "shallow/index.d.ts",
        "react-test-renderer-tests.ts"
    ]
}