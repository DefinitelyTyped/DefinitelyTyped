{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "comment-format": false,
        "dt-header": false,
        "interface-name": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-padding": false,
        "space-before-function-paren": false,
        "space-within-parens": false,
        "typedef-whitespace": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
