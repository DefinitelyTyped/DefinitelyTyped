{
    "files": [
        "index.d.ts",
        "stream-tests.ts"
    ],
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es2018",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
