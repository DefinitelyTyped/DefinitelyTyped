            "../../"
        ],
        "paths": {
            "buffer": [
                "buffer/v13"
            ],
            "console": [
                "console/v13"
            ],
            "dns": [
                "dns/v13"
            ],
            "domain": [
                "domain/v13"
            ],
            "fs": [
                "fs/v13"
            ],
            "module": [
                "module/v13"
            ],
            "net": [
                "net/v13"
            ],
            "node": [
                "node/v13"
            ],
            "node/*": [
                "node/v13/*"
            ],
            "process": [
                "process/v13"
            ],
            "querystring": [
                "querystring/v13"
            ],
            "stream": [
                "stream/v13"
            ],
            "timers": [
                "timers/v13"
            ],
            "tty": [
                "tty/v13"
            ],
            "url": [
                "url/v13"
            ],
            "util": [
                "util/v13"
            ],
            "zlib": [
                "zlib/v13"
