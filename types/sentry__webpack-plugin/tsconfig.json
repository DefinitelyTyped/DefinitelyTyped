{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "webpack": [
                "webpack/v4"
            ],
            "tapable": [
                "tapable/v1"
            ]
        },
        "types": [],
        "esModuleInterop": true,
        "paths": {
            "@sentry/*": ["sentry__*"]
        },
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "sentry__webpack-plugin-tests.ts"
    ]
}
