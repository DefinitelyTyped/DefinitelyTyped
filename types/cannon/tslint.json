{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "interface-name": false,
        "no-internal-module": false,
        "no-single-declare-module": false,
        "no-unnecessary-class": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "strict-export-declare-modifiers": false
    }
}
