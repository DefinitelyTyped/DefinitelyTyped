{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-declare-current-package": false,
        "no-internal-module": false,
        "no-single-declare-module": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
