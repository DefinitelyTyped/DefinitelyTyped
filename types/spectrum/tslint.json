{
    "extends": "dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "dt-header": false,
        "jsdoc-format": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "npm-naming": false,
        "object-literal-shorthand": false,
        "only-arrow-functions": false,
        "semicolon": false,
        "space-before-function-paren": false,
        "unified-signatures": false
    }
}
