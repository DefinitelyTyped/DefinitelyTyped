{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-redundant-jsdoc-2": false,
        "no-unnecessary-qualifier": false,
        "npm-naming": [
            true,
            {
                "errors": [
                    [
                        "NeedsExportEquals",
                        false
                    ]
                ],
                "mode": "code"
            }
        ],
        "one-variable-per-declaration": false,
        "only-arrow-functions": false,
        "prefer-const": false
    }
}
