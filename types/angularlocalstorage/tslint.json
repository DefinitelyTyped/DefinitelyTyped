{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "no-reference-import": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false,
        "whitespace": false
    }
}
