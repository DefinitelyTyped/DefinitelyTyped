{
    "extends": "dtslint/dt.json",
    "rules": {
        "callable-types": false,
        "dt-header": false,
        "export-just-namespace": false,
        "no-redundant-jsdoc": false,
        "no-trailing-whitespace": false,
        "prefer-method-signature": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}