{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "callable-types": false,
        "dt-header": false,
        "eofline": false,
        "interface-name": false,
        "no-redundant-jsdoc": false,
        "prefer-method-signature": false,
        "semicolon": false,
        "void-return": false
    }
}