{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "comment-format": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-empty-interface": false,
        "no-padding": false,
        "no-trailing-whitespace": false,
        "npm-naming": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
