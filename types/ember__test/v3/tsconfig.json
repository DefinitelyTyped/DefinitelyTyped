{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "@ember/application": ["ember__application/v3"],
            "@ember/application/*": ["ember__application/v3/*"],
            "@ember/controller": ["ember__controller/v3"],
            "@ember/engine": ["ember__engine/v3"],
            "@ember/engine/*": ["ember__engine/v3/*"],
            "@ember/object": ["ember__object/v3"],
            "@ember/object/*": ["ember__object/v3/*"],
            "@ember/routing": ["ember__routing/v3"],
            "@ember/routing/*": ["ember__routing/v3/*"],
            "@ember/service": ["ember__service/v3"],
            "@ember/test": ["ember__test/v3"],
            "@ember/test/*": ["ember__test/v3/*"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "ember__test-tests.ts"
    ]
}
