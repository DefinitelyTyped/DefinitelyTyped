{
    "private": true,
    "type": "module",
    "exports": {
        ".": {
            "import": "./build/three.module.js",
            "require": "./build/three.cjs",
            "types": "./build/three.module.d.ts"
        },
        "./examples/fonts/*": {
            "default": "./examples/fonts/*",
            "types": "./examples/fonts/*.d.ts"
        },
        "./examples/jsm/*": {
            "default": "./examples/jsm/*",
            "types": "./examples/jsm/*.d.ts"
        },
        "./addons/*": {
            "default": "./examples/jsm/*",
            "types": "./examples/jsm/*.d.ts"
        },
        "./src/*": {
            "default": "./src/*",
            "types": "./src/*.d.ts"
        },
        "./nodes": "./examples/jsm/nodes/Nodes.js"
    },
    "dependencies": {
        "fflate": "~0.6.10",
        "meshoptimizer": "~0.18.1"
    }
}
