{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "callable-types": false,
        "dt-header": false,
        "no-declare-current-package": false,
        "no-padding": false,
        "no-redundant-jsdoc": false,
        "no-single-declare-module": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "prefer-method-signature": false,
        "semicolon": false
    }
}