{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "prefer-for-of": false,
        "trim-file": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
