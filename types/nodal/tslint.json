{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "interface-name": false,
        "jsdoc-format": false,
        "member-access": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-void-expression": false,
        "prefer-const": false,
        "semicolon": false,
        "space-before-function-paren": false
    }
}
