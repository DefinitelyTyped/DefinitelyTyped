{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "dt-header": false,
        "no-redundant-jsdoc-2": false,
        "no-reference-import": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "errors": [
                    [
                        "NeedsExportEquals",
                        false
                    ]
                ],
                "mode": "code"
            }
        ],
        "prefer-const": false,
        "strict-export-declare-modifiers": false
    }
}
