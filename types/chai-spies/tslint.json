{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "jsdoc-format": false,
        "no-redundant-jsdoc-2": false,
        "no-single-declare-module": false,
        "one-variable-per-declaration": false,
        "only-arrow-functions": false,
        "prefer-const": false
    }
}
