{
    "extends": "dtslint/dt.json",
    "rules": {
        "callable-types": false,
        "dt-header": false,
        "no-padding": false,
        "no-redundant-jsdoc": false,
        "one-line": false,
        "one-variable-per-declaration": false,
        "prefer-method-signature": false,
        "semicolon": false
    }
}