{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es2015",
            "es2018.promise"
        ],
        "target": "es2015",
        "skipLibCheck": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "autoprefixer-tests.ts"
    ],
    "$schema": "https://json.schemastore.org/tsconfig"
}
