{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6", "dom"
        ],
        "jsx": "react",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "baseUrl": "../",
