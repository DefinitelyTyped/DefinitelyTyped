{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "callable-types": false,
        "dt-header": false,
        "eofline": false,
        "no-consecutive-blank-lines": false,
        "no-redundant-jsdoc": false,
        "no-trailing-whitespace": false,
        "no-unnecessary-class": false,
        "npm-naming": false,
        "prefer-method-signature": false,
        "trim-file": false
    }
}
