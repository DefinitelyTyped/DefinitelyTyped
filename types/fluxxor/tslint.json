{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "dt-header": false,
        "export-just-namespace": false,
        "no-unnecessary-generics": false,
        "no-var-keyword": false,
        "no-void-expression": false,
        "prefer-const": false,
        "void-return": false,
        "whitespace": false
    }
}
