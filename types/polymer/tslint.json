{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "no-object-literal-type-assertion": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "object-literal-shorthand": false,
        "only-arrow-functions": false,
        "prefer-const": false
    }
}
