{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "dom",
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@rails/*": ["rails__*"],
            "@hotwired/*": ["hotwired__*"]
        }
    },
    "files": [
        "index.d.ts",
        "hotwired__turbo-rails-tests.ts"
    ]
}
