{
    "files": [
        "index.d.ts",
        "react-dom-tests.ts",
        "server/index.d.ts",
        "test-utils/index.d.ts"
    ],
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "types": [],
        "paths": {
            "react-dom": ["react-dom/v15"],
            "react-dom/*": ["react-dom/v15/*"]
        },
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    }
}
