{
    "extends": "dtslint/dt.json",
    "rules": {
        // `async () => {}` is of type `() => Promise<void>` which is rejected by this rule
        "void-return": false,
        // TODOs
        "dt-header": false,
        "npm-naming": false,
        "no-unnecessary-generics": false,
        "no-unnecessary-type-assertion": false
    }
}
