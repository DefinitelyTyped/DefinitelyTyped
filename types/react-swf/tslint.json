{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-empty-interface": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-var-keyword": false,
        "one-line": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
