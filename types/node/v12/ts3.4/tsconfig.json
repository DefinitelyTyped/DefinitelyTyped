{
    "files": [
        "index.d.ts",
        "node-tests.ts"
    ],
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../../",
        "typeRoots": [
            "../../../"
        ],
        "paths": {
            "buffer": [
                "buffer/v12"
            ],
            "console": [
                "console/v12"
            ],
            "constants": [
                "constants/v12"
            ],
            "dgram": [
                "dgram/v12"
            ],
            "dns": [
                "dns/v12"
            ],
            "domain": [
                "domain/v12"
            ],
            "fs": [
                "fs/v12"
            ],
            "http": [
                "http/v12"
            ],
            "https": [
                "https/v12"
            ],
            "inspector": [
                "inspector/v12"
            ],
            "module": [
                "module/v12"
            ],
            "net": [
                "net/v12"
            ],
            "node": [
                "node/v12"
            ],
            "node/*": [
                "node/v12/*"
            ],
            "process": [
                "process/v12"
            ],
            "querystring": [
                "querystring/v12"
            ],
            "stream": [
                "stream/v12"
            ],
            "timers": [
                "timers/v12"
            ],
            "tls": [
                "tls/v12"
            ],
            "trace_events": [
                "trace_events/v12"
            ],
            "tty": [
                "tty/v12"
            ],
            "url": [
                "url/v12"
            ],
            "util": [
                "util/v12"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    }
}
