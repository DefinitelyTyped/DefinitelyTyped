{
  "extends": "dtslint/dt.json",
  "rules": {
    "ban-types": false,
    "unified-signatures": false,
    "no-empty-interface": false,
    "no-single-declare-module": false,
    "strict-export-declare-modifiers": false, // http2 needs this
    "no-unnecessary-class": false // needed for module
  }
}
