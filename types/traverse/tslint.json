{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "object-literal-shorthand": false,
        "one-line": false,
        "prefer-const": false,
        "semicolon": false,
        "space-before-function-paren": false,
        "triple-equals": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
