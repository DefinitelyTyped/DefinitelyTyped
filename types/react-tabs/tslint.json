{
    "extends": "dtslint/dt.json",
    "rules": {
        "no-any-union": false,
        "no-empty-interface": false,
        "no-object-literal-type-assertion": false,
        "no-unnecessary-generics": false,
        "no-void-expression": false,
        "strict-export-declare-modifiers": false,
        "void-return": false
    }
}
