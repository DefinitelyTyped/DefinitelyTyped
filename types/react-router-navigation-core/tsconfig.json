{
    "compilerOptions": {
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "paths": {
            "react-navigation": [
                "react-navigation/v2"
            ],
            "react-navigation/*": [
                "react-navigation/v2/*"
            ]
        },
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "jsx": "react",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true
    },
    "files": [
        "index.d.ts",
        "react-router-navigation-core-tests.tsx"
    ]
}
