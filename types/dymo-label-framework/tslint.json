{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "interface-name": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "one-line": false,
        "semicolon": false,
        "triple-equals": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
