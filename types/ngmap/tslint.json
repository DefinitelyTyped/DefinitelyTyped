{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "interface-name": false,
        "jsdoc-format": false,
        "member-access": false,
        "no-redundant-jsdoc-2": false,
        "no-single-declare-module": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "only-arrow-functions": false,
        "semicolon": false
    }
}
