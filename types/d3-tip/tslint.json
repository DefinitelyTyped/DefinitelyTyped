{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "no-consecutive-blank-lines": false,
        "no-unnecessary-generics": false,
        "npm-naming": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
