{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "@ember/object": ["ember__object"],
            "@ember/object/*": ["ember__object/*"]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "computed.d.ts",
        "core.d.ts",
        "evented.d.ts",
        "events.d.ts",
        "index.d.ts",
        "internals.d.ts",
        "mixin.d.ts",
        "observable.d.ts",
        "observers.d.ts",
        "promise-proxy-mixin.d.ts",
        "proxy.d.ts",
        "-private/types.d.ts",
        "test/lib/assert.ts",
        "test/access-modifier.ts",
        "test/core.ts",
        "test/computed.ts",
        "test/create.ts",
        "test/create-negative.ts",
        "test/detect.ts",
        "test/detect-instance.ts",
        "test/event.ts",
        "test/extend.ts",
        "test/object.ts",
        "test/observable.ts",
        "test/reopen.ts"
    ]
}
