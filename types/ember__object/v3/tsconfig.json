{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../../",
        "experimentalDecorators": true,
        "typeRoots": [
            "../../"
        ],
        "paths": {
            "@ember/application/*": ["ember__application/v3/*"],
            "@ember/array/*": ["ember__array/v3/*"],
            "@ember/component/*": ["ember__component/v3/*"],
            "@ember/debug/*": ["ember__debug/v3/*"],
            "@ember/engine/*": ["ember__engine/v3/*"],
            "@ember/object/*": ["ember__object/v3/*"],
            "@ember/routing/*": ["ember__routing/v3/*"],
            "@ember/runloop/*": ["ember__runloop/v3/*"],
            "@ember/string": ["ember__string"],
            "@ember/template/*": ["ember__template/v3/*"],
            "@ember/test/*": ["ember__test/v3/*"],
            "@ember/*": [
                "ember__*/v3"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "test/lib/assert.ts",
        "test/access-modifier.ts",
        "test/core.ts",
        "test/compat.ts",
        "test/computed.ts",
        "test/create.ts",
        "test/create-negative.ts",
        "test/detect.ts",
        "test/detect-instance.ts",
        "test/event.ts",
        "test/extend.ts",
        "test/object.ts",
        "test/observable.ts",
        "test/octane.ts",
        "test/proxy.ts",
        "test/reopen.ts"
    ]
}
