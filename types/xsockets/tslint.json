{
    "extends": "dtslint/dt.json",
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-internal-module": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "one-line": false,
        "only-arrow-functions": false,
        "semicolon": false,
        "space-before-function-paren": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false,
        "whitespace": false
    }
}
