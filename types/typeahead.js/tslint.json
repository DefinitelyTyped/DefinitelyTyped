{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "arrow-return-shorthand": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "member-access": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-single-declare-module": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "semicolon": false,
        "unified-signatures": false
    }
}
