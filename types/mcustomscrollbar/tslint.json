{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "jsdoc-format": false,
        "no-padding": false,
        "no-single-declare-module": false,
        "no-trailing-whitespace": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
