{
    "extends": "dtslint/dt.json",
    "rules": {
        "comment-format": false,
        "export-just-namespace": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-trailing-whitespace": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "one-line": false,
        "prefer-const": false,
        "semicolon": false,
        "whitespace": false
    }
}
