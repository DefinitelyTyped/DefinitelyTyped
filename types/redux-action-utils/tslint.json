{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "no-consecutive-blank-lines": false,
        "no-duplicate-imports": false,
        "no-unnecessary-generics": false,
        "no-var-keyword": false,
        "prefer-const": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
