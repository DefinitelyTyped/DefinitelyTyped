{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "interface-name": false,
        "no-duplicate-variable": false,
        "no-empty-interface": false,
        "no-unnecessary-class": false,
        "no-var-keyword": false,
        "prefer-const": false,
        "unified-signatures": false
    }
}
