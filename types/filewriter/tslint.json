{
    "extends": "dtslint/dt.json",
    "rules": {
        "dt-header": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "one-line": false,
        "prefer-const": false,
        "semicolon": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
