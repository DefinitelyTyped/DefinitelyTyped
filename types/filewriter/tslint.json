{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "one-line": false,
        "prefer-const": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
