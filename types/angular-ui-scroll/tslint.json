{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "no-inferrable-types": false,
        "no-namespace": false,
        "no-unnecessary-qualifier": false,
        "no-var-keyword": false,
        "one-variable-per-declaration": false,
        "only-arrow-functions": false,
        "prefer-const": false
    }
}
