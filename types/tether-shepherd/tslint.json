{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "dt-header": false,
        "interface-name": false,
        "max-line-length": false,
        "no-misused-new": false,
        "no-padding": false,
        "no-var-keyword": false,
        "object-literal-key-quotes": false,
        "semicolon": false,
        "unified-signatures": false
    }
}
