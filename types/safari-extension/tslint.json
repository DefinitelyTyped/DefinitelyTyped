{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "no-duplicate-variable": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "prefer-const": false,
        "prefer-for-of": false,
        "triple-equals": false,
        "jsdoc-format": false
    }
}
