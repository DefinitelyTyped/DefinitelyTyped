{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "comment-format": false,
        "jsdoc-format": false,
        "max-line-length": false,
        "no-empty-interface": false,
        "no-redundant-jsdoc-2": false,
        "semicolon": false,
        "space-within-parens": false,
        "unified-signatures": false
    }
}
