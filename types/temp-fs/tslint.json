{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "dt-header": false,
        "export-just-namespace": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "only-arrow-functions": false,
        "space-before-function-paren": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
