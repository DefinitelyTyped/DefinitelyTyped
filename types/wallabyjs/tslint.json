{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-name": false,
        "member-access": false,
        "no-object-literal-type-assertion": false,
        "no-padding": false,
        "no-redundant-jsdoc-2": false,
        "no-single-declare-module": false,
        "npm-naming": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "whitespace": false
    }
}
