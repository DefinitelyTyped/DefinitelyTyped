{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "prefer-const": false,
        "semicolon": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}
