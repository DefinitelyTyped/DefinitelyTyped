{
    "extends": "dtslint/dt.json",
    "rules": {
        "array-type": false,
        "ban-types": false,
        "callable-types": false,
        "dt-header": false,
        "jsdoc-format": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-redundant-jsdoc": false,
        "prefer-const": false,
        "prefer-method-signature": false,
        "semicolon": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}