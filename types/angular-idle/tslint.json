{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-reference-import": false,
        "npm-naming": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "trim-file": false
    }
}
