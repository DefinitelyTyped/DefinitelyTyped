{
    "extends": "../tslint.json",
    "rules": {
        "class-name": true,
        "curly": true,
        "max-line-length": false,
        "no-consecutive-blank-lines": true,
        "no-shadowed-variable": true,
        "quotemark": [
            true,
            "single"
        ],
        "align": true,
        "callable-types": false,
        "forbidden-types": false,
        "indent": [
            true,
            "spaces"
        ],
        "interface-name": false,
        "linebreak-style": [
            true,
            "LF"
        ],
        "no-empty-interface": false,
        "unified-signatures": false,
        "variable-name": [
            true,
            "check-format"
        ],
        "void-return": false
    }
}
