{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false,
        "unified-signatures": false,
        "whitespace": false
    }
}
