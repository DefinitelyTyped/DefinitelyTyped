{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "../",
    "jsx": "react",
    "typeRoots": [
      "../"
    ],
    "paths": {
      "@hcaptcha/vue-hcaptcha": [
        "hcaptcha__vue-hcaptcha"
      ]
    },
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": [
    "index.d.ts",
    "hcaptcha__vue-hcaptcha-tests.ts"
  ]
}
