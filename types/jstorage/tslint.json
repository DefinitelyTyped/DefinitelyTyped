{
    "extends": "@definitelytyped/dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "no-boolean-literal-compare": false,
        "no-consecutive-blank-lines": false,
        "no-var-keyword": false,
        "npm-naming": false,
        "semicolon": false,
        "space-within-parens": false
    }
}
