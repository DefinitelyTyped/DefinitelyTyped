    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "outDir": "build",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
    },
    "files": [
        "generate.ts"
    ]
}
